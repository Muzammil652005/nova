adow-md\"\n          >\n            <div class=\"px-4 space-y-3\">\n              <fieldset class=\"fieldset w-full\">\n                <legend class=\"fieldset-legend text-primary hidden lg:block\">\n                  Name\n                </legend>\n                <input\n                  type=\"text\"\n                  class=\"input input-primary !outline-none !shadow-none w-full\"\n                  placeholder=\"Name\"\n                />\n              </fieldset>\n\n              <fieldset class=\"fieldset w-full\">\n                <legend class=\"fieldset-legend text-primary hidden lg:block\">\n                  Phone\n                </legend>\n                <input\n                  type=\"text\"\n                  class=\"input input-primary !outline-none !shadow-none w-full\"\n                  placeholder=\"Name\"\n                />\n              </fieldset>\n\n              <fieldset class=\"fieldset w-full\">\n                <legend class=\"fieldset-legend text-primary hidden lg:block\">\n                  Email\n                </legend>\n                <input\n                  type=\"text\"\n                  class=\"input input-primary !outline-none !shadow-none w-full\"\n                  placeholder=\"Name\"\n                />\n              </fieldset>\n              <fieldset class=\"fieldset w-full\">\n                <legend class=\"fieldset-legend text-primary hidden lg:block\">\n                  Location\n                </legend>\n                <input\n                  type=\"text\"\n                  class=\"input input-primary !outline-none !shadow-none w-full\"\n                  placeholder=\"Name\"\n                />\n              </fieldset>\n\n              <fieldset class=\"fieldset w-full\">\n                <legend class=\"fieldset-legend text-primary hidden lg:block\">\n                  Message\n                </legend>\n                <textarea\n                  rows=\"3\"\n                  placeholder=\"